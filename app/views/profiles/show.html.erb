<div class="profile-head">
  <h1>My Profile</h1>
  <img src="<%= session[:profile_picture]%>" alt="">
</div>

<h2>About Me:</h2>
<div class="profile-infos"><%= @profile.info %></div>

<h2>My Posts:</h2>
<div class="posts">
  <% current_user.posts.each do |post| %>
    <%= render post %>
  <% end %>
</div>
